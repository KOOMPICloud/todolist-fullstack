# KOOMPI Provider OAuth Credentials
# These will be injected automatically when deployed on KConsole
# NEXT_PUBLIC_ prefix is required for client-side access in Next.js

NEXT_PUBLIC_KOOMPI_PROVIDER_CLIENT_ID="your_client_id_here"
NEXT_PUBLIC_KOOMPI_PROVIDER_CLIENT_SECRET="your_client_secret_here"
NEXT_PUBLIC_KOOMPI_PROVIDER_REDIRECT_URI="https://your-service-domain.tunnel.koompi.cloud/auth/callback"

# Database (SQLite at /data/db for persistence)
DATABASE_PATH="/data/db/app.db"

# App Configuration
NODE_ENV="production"
PORT=3000
